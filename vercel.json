{
    "version": 2,
    "builds": [
      {
        "src": "myWebsite/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.12"
        }
      },
      {
        "src": "requirements.txt",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/media/(.*)",
        "dest": "/media/$1"
      },
      {
        "src": "/(.*)",
        "dest": "myWebsite/wsgi.py"
      }
    ],
    "env": {
      "ALLOWED_HOSTS": ".vercel.app,127.0.0.1,localhost,muhanad-rashwan-portifolio.vercel.app",
      "DJANGO_SETTINGS_MODULE": "myWebsite.settings"
    }
}

  